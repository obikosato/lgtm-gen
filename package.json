{
  "name": "lgtm-generator",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "React + Vite環境でLGTM画像を生成するTypeScriptサンプル",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check --write src"
  },
  "keywords": [
    "lgtm",
    "typescript",
    "react",
    "vite",
    "canvas",
    "image-generation"
  ],
  "author": "obikosato",
  "license": "MIT",
  "engines": {
    "node": ">=20.19.0"
  },
  "dependencies": {
    "@chakra-ui/react": "3.27.0",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "framer-motion": "12.23.22",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "@vitejs/plugin-react": "5.0.4",
    "@biomejs/biome": "2.2.5",
    "typescript": "5.9.3",
    "vite": "7.1.9"
  }
}
